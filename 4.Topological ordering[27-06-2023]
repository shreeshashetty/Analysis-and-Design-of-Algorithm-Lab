#include<stdio.h>

int dfa[10][10],visited[10],n,expired[10],J=0;

void displayDfa(int num){
    visited[num]=-1;
    for(int i=1;i<=n;i++){
        if(dfa[num][i]==1 && visited[i]==0){
            displayDfa(i);
        }
    }
    expired[J++]=num;
}

int main(){
    printf("Enter no.of vertices\n");
    scanf("%d",&n);
    printf("Enter adjacency matrix\n");
    for(int i=1;i<=n;i++){
        for(int j=1;j<=n;j++){
            scanf("%d",&dfa[i][j]);
        }
    }
    for(int i=1;i<=n;i++){
        visited[i]=0;
    }
    int flag=0;
    for(int i=1;i<=n;i++){
            if(visited[i]==0){
                displayDfa(i);
            }
    }
    printf("\nTopological order\n");
        for(int i=9;i>=0;i--){
            if(expired[i]!=0){
                printf("%d->",expired[i]);
        }
    }
}



